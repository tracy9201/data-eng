{
  "name": "looker-vis-library",
  "version": "0.0.1",
  "description": "Looker customized visualization library",
  "scripts": {
    "start": "webpack serve --config ./webpack/client-config.js",
    "dev": "run-p pyhttps watch",
    "pyhttps": "cd dist && pyhttps",
    "watch": "webpack --watch --config ./webpack/client-config.js",
    "build": "yarn clean && webpack --config ./webpack/client-config.js --env prod",
    "clean": "rm -rf dist",
    "lint": "eslint . --fix",
    "test": "jest --config jest/config.js",
    "cov": "jest --config jest/config.js --coverage",
    "analysis": "webpack --config ./webpack/client-config.js --env prod --env analysis"
  },
  "license": "MIT",
  "dependencies": {
    "dompurify": "^2.2.7",
    "html-react-parser": "^1.2.4",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clsx": "^1.1.1",
    "compression-webpack-plugin": "^7.1.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "ignore-loader": "^0.1.2",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.3",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.11.1",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  }
}
