test: HINT_void_is_accurate_1 {
  explore_source: date_table {
    column: batch_report_summary.sum_credit_card {
      field: batch_report_summary.sum_credit_card
    }
    filters: [batch_report_summary.sales_created_at_date: "2020-05-13", practice.k_practice_id: "296"]
  }
  assert: credit_card_is_expected_value {
    expression: ${batch_report_summary.sum_credit_card} = 6140.86 ;;
  }
}
